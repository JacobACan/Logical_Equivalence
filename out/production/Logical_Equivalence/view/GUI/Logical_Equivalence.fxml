<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="view.GUI.Controller"
            prefHeight="400" prefWidth="NaN"
            styleClass="centered"
            fx:id="scene">
    <VBox fx:id="container" styleClass="centered">
        <Label fx:id="title" styleClass="title" text="Logical Equivalence"></Label>
        <HBox styleClass="centered">
            <TextField minWidth="400" promptText="Equation 1 (Ex: (p or q))"  styleClass="equation"  fx:id="equation1" onKeyTyped="#setEquation1" ></TextField>
            <TextField minWidth="400" promptText="Equation 2 (Ex: (p iff q))"  styleClass="equation"  fx:id="equation2" onKeyTyped="#setEquation2"></TextField>
        </HBox>
        <VBox  fx:id="result"></VBox>
    </VBox>

</AnchorPane>
